{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-console": "off",
      "max-len": "off",
      "func-names": "off",
      "no-underscore-dangle": "off",
      "no-use-before-define": "off",
      "no-param-reassign": "off",
      "consistent-return": "off",
      "quotes": [0, "single"],
      "linebreak-style": 0
    }
}